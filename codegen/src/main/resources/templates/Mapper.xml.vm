<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="${base.pkg}.${base.module}.mapper.${base.className}Mapper">

  <resultMap id="${base.className}Map" type="${base.pkg}.${base.module}.entity.${base.className}">
    #foreach($column in $columns)
      #if($$column.priKey)
        <id property="${column.lowerAttrName}" column="${column.columnName}"/>
      #else
        <result property="${column.lowerAttrName}" column="${column.columnName}"/>
      #end
    #end
  </resultMap>
</mapper>
